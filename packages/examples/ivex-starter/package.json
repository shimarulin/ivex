{
  "name": "ivex-starter",
  "version": "0.1.0",
  "description": "iVex starter example",
  "author": "Vyacheslav Shimarulin <shimarulin@gmail.com>",
  "homepage": "https://github.com/shimarulin/ivex/tree/master/packages/examples/ivex-starter#readme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shimarulin/ivex.git"
  },
  "bugs": {
    "url": "https://github.com/shimarulin/ivex/issues"
  },
  "scripts": {
    "serve": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.vue": [
        "eslint --fix",
        "stylelint --fix",
        "git add"
      ],
      "**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "**/*.{less,css}": [
        "stylelint --fix",
        "git add"
      ],
      "**/*.md": [
        "prettier --parser markdown --prose-wrap always --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "nuxt": "^2.4.0"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "@platr/eslint-config-vue": "^0.1.0",
    "eslint": "^5.12.1",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.2",
    "stylelint": "^9.10.1",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0"
  }
}
